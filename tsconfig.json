{
  "target": "esnext",
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "jsx": "preserve",
    "importsNotUsedAsValues": "preserve",
    "preserveValueImports": false,
    "strictNullChecks": false,
    "noImplicitAny": false
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
